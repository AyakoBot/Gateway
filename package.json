{
  "name": "@ayako/gateway",
  "version": "2.12.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "swc src -d dist --strip-leading-paths --copy-files",
    "dev": "pnpm build && node --enable-source-maps --experimental-wasm-modules --experimental-json-modules ./dist/index.js --debug --warn --dev --debug-db --local",
    "lint": "pnpx eslint 'src/**/*.ts' --fix",
    "run": "node --max-old-space-size=128 --no-deprecation --no-warnings --experimental-json-modules --inspect=0.0.0.0:9221 ./dist/index.js",
    "start": "pnpm run run",
    "watch": "swc src -d dist --strip-leading-paths --copy-files --watch"
  },
  "dependencies": {
    "@discordjs/core": "^2.4.0",
    "@discordjs/rest": "^2.6.0",
    "@discordjs/ws": "^2.0.4",
    "discord-api-types": "0.38.37",
    "discord-hybrid-sharding": "^3.0.1",
    "dotenv": "^17.2.3",
    "glob": "^13.0.0",
    "ioredis": "^5.9.1",
    "node-schedule": "^2.1.1",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@babel/eslint-plugin": "^7.27.1",
    "@eslint/compat": "^2.0.1",
    "@eslint/js": "^9.39.2",
    "@swc/cli": "^0.7.9",
    "@swc/core": "1.15.8",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/glob": "^9.0.0",
    "@types/node": "^25.0.6",
    "@types/node-schedule": "^2.1.8",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "packageManager": "pnpm@10.18.2",
  "engines": {
    "node": "^24.0.1",
    "pnpm": "^10.18.2"
  }
}
