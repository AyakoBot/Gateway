{
  "name": "@ayako/gateway",
  "version": "2.12.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "swc src -d dist --strip-leading-paths --copy-files",
    "dev": "node --enable-source-maps --experimental-wasm-modules --experimental-json-modules ./dist/index.js --debug --warn --dev --debug-db",
    "lint": "pnpx eslint 'src/**/*.ts' --fix",
    "run": "node --max-old-space-size=2048 --no-deprecation --no-warnings --experimental-json-modules ./dist/index.js",
    "start": "pnpm run run",
    "watch": "swc src -d dist --strip-leading-paths --copy-files --watch"
  },
  "dependencies": {
    "@discordjs/core": "^2.2.2",
    "@discordjs/rest": "^2.6.0",
    "@discordjs/ws": "^2.0.3",
    "discord-api-types": "0.38.26",
    "discord-hybrid-sharding": "^3.0.1",
    "dotenv": "^17.2.2",
    "glob": "^11.0.3",
    "ioredis": "^5.8.0",
    "longjohn": "^0.2.12",
    "node-schedule": "^2.1.1",
    "os": "^0.1.2",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.4",
    "@babel/eslint-plugin": "^7.27.1",
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.36.0",
    "@swc/cli": "^0.7.8",
    "@swc/core": "1.13.5",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/glob": "^9.0.0",
    "@types/node": "^24.5.2",
    "@types/node-schedule": "^2.1.8",
    "@types/source-map-support": "^0.5.10",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "tsx": "^4.20.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.44.1"
  },
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": "^24.0.1",
    "pnpm": "^10.17.1"
  }
}
